<?xml version="1.0" encoding="ISO-8859-1"?><riscoss>
    <model>
        <relationships>
            <satisfy source="fuzzified:missingUserManual" target="weakdescription"/>
            <satisfy source="fuzzified:bugsPerYear,fuzzified:numberOfSecurityBugs,fuzzified:pastImplementationTime,fuzzified:pastResponseTime,fuzzified:pastTimeToUpdate" target="criticalSecurityIssuesHistory" function="fx.join.or(sources)"/>
            <satisfy source="fuzzified:missingSecurityTests" target="testingShortfalls"/>
            <satisfy source="testingShortfalls" target="lateSecurityBugsDetection"/>
            <satisfy weight="0.5" source="fuzzified:noExistingThreatAnalysis,fuzzified:noSecurityFeaturesArchitecture,fuzzified:noSystematicBugHandling" target="lowSecurityRiskAwareness" function="fx.join.avg(sources)"/>
            <satisfy source="fuzzified:numberOfSecurityBugs,lowSecurityRiskAwareness,testingShortfalls" target="presenceOfSecurityBugs" function="fx.join.or(sources)"/>
            <satisfy source="lowSecurityRiskAwareness" target="degradedSecurityFeatures"/>
            <satisfy source="fuzzified:newCommunicationInterfaces,presenceOfSecurityBugs" target="possibilityToObtainLogTraceData" function="fx.join.avg(sources)"/>
            <expose source="uncertaintyAboutQuality,sub_lackInSWQuality" target="lackInSWQuality" function="fx.join.avg(sources)"/>
            <expose weight="1.0" source="degradedSecurityFeatures,lateSecurityBugsDetection,presenceOfSecurityBugs,weakdescription" target="sub_lackInSWQuality" function="fx.join.or(sources)"/>
            <expose source="lackInSWQuality" target="ModifyDestroyData"/>
            <expose source="BreakSecurityPolicies,IntroduceSecuritysensitiveData,ModifyDestroyData,ObtainLogTraceConfidentialData,ObtainUnauthorizedAccess" target="SecurityRisk" function="fx.join.avg(sources)"/>
            <expose source="possibilityToObtainLogTraceData" target="event_possibilityToObtainLogTraceData"/>
            <expose weight="0.8" source="fuzzified:noExistingRiskAnalyses,fuzzified:othersLowConfidence" target="sub_uncertaintyAboutQuality" function="fx.join.avg(sources)"/>
            <expose source="fuzzified:bugsPerYear,fuzzified:numberOfSecurityBugs,fuzzified:pastImplementationTime,fuzzified:pastResponseTime,fuzzified:pastTimeToUpdate" target="TEI:PresenceOfSecurityVulnerabilities" function="fx.join.avg(sources)"/>
            <expose source="criticalSecurityIssuesHistory,sub_uncertaintyAboutQuality" target="uncertaintyAboutQuality" function="fx.join.or(sources)"/>
            <expose source="lackInSWQuality" target="ObtainUnauthorizedAccess"/>
            <expose source="fuzzified:noExistingRiskAnalyses,fuzzified:noSecurityFeaturesArchitecture" target="TEI:LackOfRiskAwareness" function="fx.join.avg(sources)"/>
            <expose source="lackInSWQuality,event_possibilityToObtainLogTraceData" target="ObtainLogTraceConfidentialData" function="fx.join.and(sources)"/>
            <expose source="lackInSWQuality" target="HumanErrorOmission"/>
            <expose source="fuzzified:missingSecurityTests,fuzzified:missingUserManual,fuzzified:noExistingThreatAnalysis,fuzzified:noSystematicBugHandling,fuzzified:othersLowConfidence" target="TEI:lackOfSystematicProcess" function="fx.join.avg(sources)"/>
            <expose source="possibilityToObtainLogTraceData" target="IntroduceSecuritysensitiveData"/>
            <expose source="HumanErrorOmission,possibilityToObtainLogTraceData" target="BreakSecurityPolicies" function="fx.join.avg(sources)"/>
            <indicate source="noSecurityFeaturesArchitecture" target="fuzzified:noSecurityFeaturesArchitecture"/>
            <indicate source="pastImplementationTime" target="fuzzified:pastImplementationTime"/>
            <indicate source="missingUserManual" target="fuzzified:missingUserManual"/>
            <indicate source="noSystematicBugHandling" target="fuzzified:noSystematicBugHandling"/>
            <indicate source="noExistingThreatAnalysis" target="fuzzified:noExistingThreatAnalysis"/>
            <indicate source="newCommunicationInterfaces" target="fuzzified:newCommunicationInterfaces"/>
            <indicate source="othersLowConfidence" target="fuzzified:othersLowConfidence"/>
            <indicate source="pastResponseTime" target="fuzzified:pastResponseTime"/>
            <indicate source="bugsPerYear" target="fuzzified:bugsPerYear"/>
            <indicate source="noExistingRiskAnalyses" target="fuzzified:noExistingRiskAnalyses"/>
            <indicate source="pastTimeToUpdate" target="fuzzified:pastTimeToUpdate"/>
            <indicate source="missingSecurityTests" target="fuzzified:missingSecurityTests"/>
            <indicate source="numberOfSecurityBugs" target="fuzzified:numberOfSecurityBugs"/>
        </relationships>
        <entities>
            <indicator id="bugsPerYear">
                <property name="input" value="true"/>
                <property name="description" value="Bugs opened per year (obtainable from bug-trackers)"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="pastResponseTime">
                <property name="input" value="true"/>
                <property name="description" value="Response time for bug assignment (obtainable from bug-trackers)"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="pastTimeToUpdate">
                <property name="input" value="true"/>
                <property name="description" value="Time to update to new versions"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="numberOfSecurityBugs">
                <property name="input" value="true"/>
                <property name="description" value="Number of new bugs marked as security bugs, or security-related bugs (obtainable with limitations from bug-trackers)"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="pastImplementationTime">
                <property name="input" value="true"/>
                <property name="description" value="Avg. time to implement (thus close) a bug. Gives an indicator of the responsiveness of the community to security bugs. (obtainable from bug-trackers)"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="missingSecurityTests">
                <property name="input" value="true"/>
                <property name="description" value="Does there exist some information on security-related tests and how though are they?"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="missingUserManual">
                <property name="input" value="true"/>
                <property name="description" value="Is a high quality and up-to date description/manuals/architectural description etc. available?"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="noSystematicBugHandling">
                <property name="input" value="true"/>
                <property name="description" value="Does the community have a systematic process of bug handling, are there guidelines available, are these guidelines followed in the issue tracker?"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="noExistingThreatAnalysis">
                <property name="input" value="true"/>
                <property name="description" value="Does an analysis of known threats (own or 3rd party) exist for this component?"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="noSecurityFeaturesArchitecture">
                <property name="input" value="true"/>
                <property name="description" value="Is the architecture security-driven, built with security in mind, properly documented, and properly respected while evolving the component? "/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="newCommunicationInterfaces">
                <property name="input" value="true"/>
                <property name="description" value="Does the component introduce new communication interfaces into the software? They could be possibly misused for obtaining or storing security-relevant data."/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="othersLowConfidence">
                <property name="input" value="true"/>
                <property name="description" value="How high is the peers/competitors confidence on this software? How often is it installed, how is it commented in forums, is it known to be reliable, etc.?"/>
                <property name="datatype" value="real"/>
            </indicator>
            <indicator id="noExistingRiskAnalyses">
                <property name="input" value="true"/>
                <property name="description" value="Does a specific risk analysis (own or 3rd party) exist for this component?"/>
                <property name="datatype" value="real"/>
            </indicator>
            <event id="lackInSWQuality">
                <property name="description" value="A known lack in software quality (security-related and not, both actually and historically) increases security risk."/>
                <property name="output" value="true"/>
            </event>
            <event id="sub_lackInSWQuality">
                <property name="output" value="false"/>
            </event>
            <event id="SecurityRisk">
                <property name="description" value="Root risk of the model. Combines a variety of security-related issues."/>
                <property name="output" value="true"/>
            </event>
            <event id="ModifyDestroyData">
                <property name="description" value="Modification or destruction of data by a (human or software) attacker."/>
                <property name="output" value="true"/>
            </event>
            <event id="ObtainUnauthorizedAccess">
                <property name="description" value="A malicious user gets unauthorized access to a system."/>
                <property name="output" value="true"/>
            </event>
            <event id="ObtainLogTraceConfidentialData">
                <property name="description" value="Confidential data is obtained/logged/traced/disclosed by the component in an unmanaged way. This is a critical security threat and may be probihited by law."/>
                <property name="output" value="true"/>
            </event>
            <event id="BreakSecurityPolicies">
                <property name="description" value="Security policies are broken by the implementation or the usage processes."/>
                <property name="output" value="true"/>
            </event>
            <event id="IntroduceSecuritysensitiveData">
                <property name="description" value="New security-sensitive data is introduced and possibly saved by the component."/>
                <property name="output" value="true"/>
            </event>
            <event id="HumanErrorOmission">
                <property name="description" value="Human error and ommission are critical for security. They are increased mainly by lacks in software quality."/>
                <property name="output" value="true"/>
            </event>
            <event id="uncertaintyAboutQuality">
                <property name="description" value="Uncertainty about software quality is driven by the past ways of handling critical issues and on 3rd party knowledge of the quality."/>
                <property name="output" value="true"/>
            </event>
            <event id="sub_uncertaintyAboutQuality">
                <property name="output" value="false"/>
            </event>
            <event id="event_possibilityToObtainLogTraceData">
                <property name="output" value="false"/>
            </event>
            <event id="TEI:PresenceOfSecurityVulnerabilities">
                <property name="output" value="true"/>
            </event>
            <event id="TEI:lackOfSystematicProcess">
                <property name="output" value="true"/>
            </event>
            <event id="TEI:LackOfRiskAwareness">
                <property name="output" value="true"/>
            </event>
            <situation id="lateSecurityBugsDetection"/>
            <situation id="degradedSecurityFeatures"/>
            <situation id="presenceOfSecurityBugs"/>
            <situation id="possibilityToObtainLogTraceData"/>
            <situation id="testingShortfalls"/>
            <situation id="weakdescription"/>
            <situation id="lowSecurityRiskAwareness"/>
            <situation id="fuzzified:bugsPerYear"/>
            <situation id="fuzzified:pastResponseTime"/>
            <situation id="fuzzified:numberOfSecurityBugs"/>
            <situation id="fuzzified:pastImplementationTime"/>
            <situation id="fuzzified:noSystematicBugHandling"/>
            <situation id="fuzzified:noExistingThreatAnalysis"/>
            <situation id="fuzzified:noSecurityFeaturesArchitecture"/>
            <situation id="fuzzified:othersLowConfidence"/>
            <situation id="fuzzified:noExistingRiskAnalyses"/>
            <situation id="criticalSecurityIssuesHistory"/>
            <situation id="fuzzified:newCommunicationInterfaces"/>
            <situation id="fuzzified:missingUserManual"/>
            <situation id="fuzzified:missingSecurityTests"/>
            <situation id="fuzzified:pastTimeToUpdate"/>
        </entities>
    </model>
</riscoss>
